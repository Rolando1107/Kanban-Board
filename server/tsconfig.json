{
  "extends": "../tsconfig.json",
    "compilerOptions": {
      "target": "ES2020",
      "module": "Node16",
      "rootDir": "./src",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "Node16"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }